				spr(7+i+16*j,16+8*i+game.camoffset,16+8*j)
			end
		end
		spr(6,80+game.camoffset,12)

		-- city (background)
		for i=0,8 do
			for j=0,3 do
				local x = 88+8*i+prlxspd*game.camoffset
				if(x >= game.camoffset - 8 and x <= game.camoffset + 136) then
					spr(64+i+16*j,x,74+8*j)
				end
			end
		end

		for i=0,8 do
			for j=0,3 do
				local x = 88*3+8*i+prlxspd*game.camoffset
				if(x >= game.camoffset - 8 and x <= game.camoffset + 136) then
					spr(64+i+16*j,x,74+8*j)
				end
			end
		end
		
		-- blocks
		for b in all(blocks) do
			if(b.x > player.x - 128 and b.x < player.x + 128) then
				spr(b.spr,b.x,b.y)
			end
		end

		-- player health bar
		for i=1,player.curhealth do
			rectfill(game.camoffset+8,40+6*i,game.camoffset+16,42+6*i,8)
		end
		for i=player.curhealth+1,player.maxhealth do
			rectfill(game.camoffset+8,40+6*i,game.camoffset+16,42+6*i,7)
		end

		-- player shots
		if(not player.isdead) then
			for s in all(player.shots) do
				if(s.numframes == 1) then
					spr(s.spr,s.x,s.y,s.w/8,s.h/8,s.xrev)
				else
					spr(s.spr+s.frameoffset*(game.timer%s.numframes),s.x,s.y,s.w/8,s.h/8,s.xrev)
				end
			end
		end

		-- enemy table
		for e in all(enemies) do
			if(e.spr == 128 and player.x > e.x + e.w/2) then
				spr(e.spr,e.x,e.y,e.w/8,e.h/8,true)
			else
				spr(e.spr,e.x,e.y,e.w/8,e.h/8)
			end
		end

		-- boss
		if(game.iscamfixed and boss.isalive) then
			for i=0,4 do
				for j=0,9 do
					spr(132+j+i*16,boss.x+j*8,boss.y+i*8)
				end
			end
			if(boss.phase == 2 and boss.timer < 150) then
				spr(75+flr((game.timer%6)/2),boss.x-4,boss.y+3)
			end
		end

		-- enemy shots table
		for s in all(enemyshots) do
			spr(s.spr,s.x,s.y,s.w/8,s.h/8)
		end

		-- player
		if(not player.isdead) then
			if(not player.isinv or (player.isinv and game.timer % 2 == 0)) then
				if(player.dir == "left") then
					spr(player.spr,player.x,player.y,1,2,true)
				elseif(player.dir == "right") then
					spr(player.spr,player.x,player.y,1,2)
				end

				-- player charging animation
				if(player.chargelv >= 1) then
					for p in all(player.chrgeani) do
						rect(p.x,p.y,p.x,p.y,p.col)
					end
				end
			end
		end

		-- player death animation
		if(player.isdead) then
			for o in all(player.deathani) do
				spr(1,o.x,o.y,1/2,1/2)
			end
			print("game over",48+game.camoffset,64,7)
		end

		if(boss.isalive == false) then
			print("you win!",48+game.camoffset,64,7)
		end
	end
end
