pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function collision(o1,o2)
	if( o1.x + o1.w > o2.x and o1.x < o2.x + o2.w
	and o1.y + o1.h > o2.y and o1.y < o2.y + o2.h) then
		return true
	else
		return false
	end
end

function playerdeathani()
	for i=0,7 do
		local o = {}
		o.x = player.x + 4
		o.y = player.y + 8
		o.angle = i * 45
		o.spd = 2
		add(player.deathani,o)
	end
end

function playerdamaged()
	if not(player.isinv) then
		sfx(1)
		player.curhealth = player.curhealth - 1
		player.isinv = true
		if(player.curhealth <= 0) then
			player.isdead = true
			playerdeathani()
		end
	end
end

function spwnblck(i,y)
	for j=0,1 do
		local b = {}
		b.x = i*16+j*8
		b.y = y
		b.w = 8
		b.h = 8
		b.spr = 9+j
		add(blocks,b)
	end
end

function initgame()
	-- initialize the player
	player = {}
	player.x = 32
	player.y = 104
	player.w = 8
	player.h = 16
	player.dy = 0
	player.numjumps = 0
	player.jumpspd = -3.5
	player.dir = "right"
	player.shots = {}
	player.dshtimer = 0
	player.dshtype = nil
	player.numshots = 0

	-- player charge ability
	player.ischarging = false
	player.chargetimer = 0
	player.chargelv = -1
	player.chrgeani = {}

	player.spd = 1.5
	player.curhealth = 6
	player.maxhealth = 6
	player.isdead = false
	player.isonground = true
	player.spr = 11
	player.walktimer = 0

	-- player invincibility and death
	player.deathani = {}
	player.isinv = false
	player.invtimer = 0
	player.deathtimer = 0
	
	local plat1 = 120
	local plat2 = 100
	local plat3 = 88

	-- spawn blocks for the player to walk on
	blocks = {}
	for i=0,20 do
		spwnblck(i,plat1)
	end
	for i=22,30 do
		spwnblck(i,plat1)
	end
	for i=32,40 do
		spwnblck(i,plat1)
	end
	for i=43,60 do
		spwnblck(i,plat1)
	end
	for i=63,79 do
		spwnblck(i,plat1)
	end

	for i=20,24 do
		spwnblck(i,plat3)
	end

	for i=72,72 do
		spwnblck(i,plat2)
	end

	-- create templates for the two enemy types
	enemies = {}
	enemyshots = {}
	local e1 = {}
	e1.w = 8
	e1.h = 16
	e1.spr = 128
	e1.timer = 20
	e1.health = 6
	local e2 = {}
	e2.w = 16
	e2.h = 12
	e2.spr = 129
	e2.timer = 0
	e2.health = 8

	-- add enemies to the game
	add(enemies,{x=16*10,y=plat1-e2.h,w=e2.w,h=e2.h,spr=e2.spr,timer=e2.timer,health = e2.health})
	add(enemies,{x=16*20,y=plat1-e1.h,w=e1.w,h=e1.h,spr=e1.spr,timer=e1.timer,health = e1.health})
	add(enemies,{x=16*20,y=plat3-e2.h,w=e2.w,h=e2.h,spr=e2.spr,timer=e2.timer,health = e2.health})
	add(enemies,{x=16*30,y=plat1-e2.h,w=e2.w,h=e2.h,spr=e2.spr,timer=e2.timer,health = e2.health})
	add(enemies,{x=16*35,y=plat1-e2.h,w=e2.w,h=e2.h,spr=e2.spr,timer=e2.timer,health = e2.health})
	add(enemies,{x=16*50,y=plat1-e1.h,w=e1.w,h=e1.h,spr=e1.spr,timer=e1.timer,health = e1.health})
	add(enemies,{x=16*55,y=plat1-e1.h,w=e1.w,h=e1.h,spr=e1.spr,timer=e1.timer,health = e1.health})
	add(enemies,{x=16*60,y=plat1-e1.h,w=e1.w,h=e1.h,spr=e1.spr,timer=e1.timer,health = e1.health})
	
	-- set game variables
	game.camoffset = 0
	game.lvlen = 128 * 10

	game.timer = 0
	game.gravity = 0.3
	game.iscamfixed = false

	-- boss
	boss = {}
	boss.phase = 0
	boss.x = 80*16
	boss.y = 88
	boss.w = 9*8
	boss.h = 4*8
	boss.timer = 0
	boss.health = 60
	boss.maxhealth = 60
	boss.isalive = true
end

function _init()

	-- initialize the game
	game = {}

	-- start the game at the title screen
	game.state = "title"
	game.version = "v1.0"
end

function _update()
	if(game.state == "title") then
		if(btnp(4)) then
			game.state = "story"
		end
	elseif(game.state == "story") then
		if(btnp(4)) then
			game.state = "tutorial"
		end
	elseif(game.state == "tutorial") then
		if(btnp(4)) then
			game.state = "game"
			initgame()
		end
	elseif(game.state == "game") then
		if(not player.isdead) then
			-- player i frames
			if(player.isinv) then
				player.invtimer = player.invtimer + 1
				if(player.invtimer > 60) then
					player.isinv = false
					player.invtimer = 0
				end
			end

			-- player movement
			if(btn(0)) then
				player.dx = -player.spd
				player.dir = "left"
				if(player.isonground) then
					player.walktimer = player.walktimer + 0.4
					if(player.walktimer > 3) then
						player.walktimer = 0
					end
				end
				player.spr = 11 + flr(player.walktimer)
			elseif(btn(1)) then
				player.dx = player.spd
				player.dir = "right"
				if(player.isonground) then
					player.walktimer = player.walktimer + 0.4
					if(player.walktimer > 3) then
						player.walktimer = 0
					end
				end
				player.spr = 11 + flr(player.walktimer)
			end
			if not(btn(0)) and not(btn(1)) then
				player.dx = 0
				player.walktimer = 0
				player.spr = 11
			end

			-- player jump
			if(btnp(2) and player.numjumps < 2) then
				player.numjumps = player.numjumps + 1
				player.dy = player.jumpspd
				player.walktimer = 0
				player.spr = 11
			end

			-- player releases jump
			if(player.dy < 0 and not btn(2)) then
				player.dy = 0
			end

			if(player.numjumps == 0 and player.dy > 0) then
				player.numjumps = 1
			end

			-- player shot ability
			if(btnp(4) and player.numshots < 3 and not player.ischarging) then
				local shtspd = 4
				local s = {}
				s.y = player.y+2
				s.w = 4
				s.h = 4
				s.dmg = 1
				s.spr = 1
				s.numframes = 1
				sfx(0)
				if(player.dir == "right") then
					s.x = player.x+5
					s.spd = shtspd
					add(player.shots,s)
				else
					s.x = player.x-1
					s.spd = -shtspd
					add(player.shots,s)
				end
				player.numshots = player.numshots + 1
				player.ischarging = true
			end

			-- if the player is charging
			if(player.ischarging) then
				player.chargetimer = player.chargetimer + 1
				if(player.chargetimer == 10) then
					player.chargelv = 0
				elseif(player.chargetimer == 20) then
					player.chargelv = 1
				elseif(player.chargetimer == 50) then
					player.chargelv = 2
				end

				if(player.chargelv >= 1) then
					if(player.chargetimer % 2 == 0) then
						local p = {}
						p.initoffset = flr(rnd(8))
						p.x = p.initoffset + player.x
						p.offsety = 0
						p.y = player.y + 16 + p.offsety
						p.spd = -1 * player.chargelv
						if(player.chargelv == 1) then
							p.col = 11
						elseif(player.chargelv == 2) then
							p.col = 12
						end
						p.timer = 0
						add(player.chrgeani,p)
					end
				end
			end

			-- player charging animation
			for p in all(player.chrgeani) do
				p.x = p.initoffset + player.x
				p.offsety = p.offsety + p.spd
				p.y = player.y + 16 + p.offsety
				

				p.timer = p.timer + 1
				if(p.timer > 15 / player.chargelv) do
					del(player.chrgeani,p)
				end
			end

			-- player releases charge
			if (not btn(4) and player.ischarging) then
				player.ischarging = false
				player.chargetimer = 0

				if(player.chargelv >= 0) then
					player.numshots = player.numshots + 1

					-- player shoots a shot based on the charge level
					local shtspd = 4
					local s = {}
					s.numframes = 1
					sfx(0)
					if(player.chargelv == 0) then
						s.y = player.y+2
						s.w = 4
						s.h = 4
						s.spr = 1
						s.dmg = 1
						if(player.dir == "right") then
							s.x = player.x+5
							s.spd = shtspd
							add(player.shots,s)
						else
							s.x = player.x-1
							s.spd = -shtspd
							add(player.shots,s)
						end
					elseif(player.chargelv == 1) then
						s.y = player.y+2
						s.w = 8
						s.h = 5
						s.spr = 17
						s.dmg = 2
						if(player.dir == "right") then
							s.x = player.x+5
							s.spd = shtspd
							add(player.shots,s)
						else
							s.x = player.x-1
							s.spd = -shtspd
							add(player.shots,s)
						end
					elseif(player.chargelv == 2) then
						s.y = player.y
						s.w = 16
						s.h = 8
						s.spr = 2
						s.numframes = 3
						s.frameoffset = 16
						s.dmg = 4
						if(player.dir == "right") then
							s.x = player.x+5
							s.spd = shtspd
							add(player.shots,s)
						else
							s.x = player.x-10
							s.spd = -shtspd
							s.xrev = true
							add(player.shots,s)
						end
					end
				end

				player.chargelv = -1
			end

			-- player shot update
			for s in all(player.shots) do
				s.x = s.x + s.spd

				-- if the bullet collides with an enemy
				for e in all(enemies) do
					if(collision(s,e)) then
						player.numshots = player.numshots - 1
						e.health = e.health - s.dmg
						sfx(1)

						-- level 0 shots never pierce
						if(s.dmg == 1) then
							del(player.shots,s)
						end

						-- otherwise, if the enemy is killed, level 1 and 2 shots
						-- pierce
						if(e.health <= 0) then
							del(enemies,e)
							if(flr(rnd(4)) == 0 and player.curhealth < player.maxhealth) then
								player.curhealth = player.curhealth + 1
							end
						else
							del(player.shots,s)
						end
					end
				end
				if(s.x > 132 + game.camoffset or s.x < -4 + game.camoffset) then
					del(player.shots,s)
					player.numshots = player.numshots - 1
				end

				-- boss collision detection
				local b = {}
				b.x = boss.x + 16
				b.y = boss.y
				b.w = boss.w - 16
				b.h = boss.h

				if(collision(s,b) and boss.phase > 0 and boss.isalive) then
					boss.health = boss.health - s.dmg
					del(player.shots,s)
					sfx(1)
					player.numshots = player.numshots - 1
					if(boss.health <= 0) then
						boss.isalive = false
						enemyshots = {}
					end
				end
			end
			if(player.numshots < 0) then
				player.numshots = 0
			elseif(player.numshots > 3) then
				player.numshots = 3
			end

			-- player collision detection
			player.x = player.x + player.dx
			player.y = player.y + player.dy
			if(player.x < game.camoffset) then
				player.x = game.camoffset
			end
			if(player.x > game.lvlen - player.w - 1) then
				player.x = game.lvlen - player.w - 1
			end
			if(player.y > 128) then
				player.isdead = true
				playerdeathani()
				player.curhealth = 0
			end
			for b in all(blocks) do
				player.isonground = false
				local p = {}
				p.x = player.x
				p.y = player.y + 15
				p.w = player.w
				p.h = 2
				local bcol = {}
				bcol.x = b.x
				bcol.y = b.y
				bcol.w = b.w
				bcol.h = 4
				if(collision(p,bcol) and player.dy >= 0) then
					player.y = b.y - player.h
					player.dy = 0
					player.numjumps = 0
					player.isonground = true
					break
				end
			end
			for e in all(enemies) do
				if(collision(player,e) and not player.isinv) do
					playerdamaged()
				end
			end

			if(player.isonground == false) then
				player.dy = player.dy + game.gravity
			end
		else
			-- player death animation
			if(player.isdead) then
				for o in all(player.deathani) do
					o.x = o.x + o.spd * cos(o.angle/360)
					o.y = o.y + o.spd * sin(-o.angle/360)
				end
				player.deathtimer = player.deathtimer + 1
				if(player.deathtimer > 150) then
					initgame()
				end
			end
		end

		-- enemy shot update
		for s in all(enemyshots) do
			s.x = s.x + s.spd
			if(s.seeking == true) then
				if(player.y + player.h/2 > s.y + s.h/2) then
					s.y = s.y + 0.5
				elseif(player.y + player.h/2 < s.y + s.h/2) then
					s.y = s.y - 0.5
				end
			end

			-- if the bullet collides with the player
			if(not player.isdead) then
				if(collision(s,player) and not player.isinv) then
					del(enemyshots,s)
					playerdamaged()
				end
			end
			if(s.x > 132 + game.camoffset or s.x < -4 + game.camoffset) then
				del(enemyshots,s)
			end
		end

		-- camera movement
		if not(game.iscamfixed) then
			if(player.x > 64 and player.x < game.lvlen - 64) then
				camera(player.x-64,0)
				game.camoffset = player.x - 64
			elseif(player.x <= 64) then
				camera(0,0)
				game.camoffset = 0
			elseif(player.x >= game.lvlen - 64) then
				camera(game.lvlen - 128,0)
				game.camoffset = game.lvlen - 128
				game.iscamfixed = true
				sfx(2)
			end
		end

		-- update enemies (onscreen only)
		for e in all(enemies) do
			if(e.x > game.camoffset-4 and e.x < game.camoffset+128+4) then
				e.timer = e.timer + 1
				if(e.timer >= 45) then
					if(e.spr == 128) then
						local s = {}
						if(player.x > e.x + e.w/2) then
							s.x = e.x+2+e.w
							s.spd = 2
						else
							s.x = e.x-2
							s.spd = -2
						end
						s.y = e.y+3
						s.w = 4
						s.h = 4
						s.spr = 1
						add(enemyshots,s)
						e.timer = 0
						sfx(0)
					elseif(e.spr == 129) then
						local s = {}
						s.x = e.x-7
						s.y = e.y+1
						s.w = 8
						s.h = 5
						s.spr = 17
						s.spd = -2
						add(enemyshots,s)
						e.timer = 15
						sfx(0)
					end
				end
			else
				if(e.spr == 128) then
					e.timer = 20
				else
					e.timer = 0
				end
			end
		end

		-- update boss
		if(game.iscamfixed and boss.isalive) then

			-- after the intro phase
			if(boss.phase > 0) then

				-- boss collision detection
				local b = {}
				b.x = boss.x + 16
				b.y = boss.y
				b.w = boss.w - 16
				b.h = boss.h

				if(collision(player,b)) then
					playerdamaged()
				end

				-- update the boss' timer
				if(boss.health / boss.maxhealth > 0.5) then
					boss.timer = (boss.timer + 1)  % 32000
				else
					boss.timer = (boss.timer + 2)  % 32000
					if(boss.timer % 2 == 1) then
						boss.timer = boss.timer + 1
					end
				end
			end

			-- boss introduction
			if(boss.phase == 0) then
				boss.x = boss.x - 0.5
				sfx(0,-2)
				if(boss.x <= 75.5*16) then
					boss.x = 75.5*16
					boss.phase = 1
					sfx(2,-2)
				end

			-- phase 1 (2x small laser shots followed by a large laser shot)
			elseif(boss.phase == 1) then
				if(boss.timer == 30 or boss.timer == 60) then
					local s = {}
					s.x = boss.x
					s.y = boss.y + 5
					s.w = 8
					s.h = 4
					if(boss.health / boss.maxhealth > 0.5) then
						s.spd = -2
					else
						s.spd = -4
					end
					s.spr = 160
					add(enemyshots,s)
					sfx(0)
				elseif(boss.timer == 90) then
					local s = {}
					s.x = boss.x
					s.y = boss.y + 5
					s.w = 16
					s.h = 4
					if(boss.health / boss.maxhealth > 0.5) then
						s.spd = -2
					else
						s.spd = -4
					end
					s.spr = 161
					add(enemyshots,s)
					sfx(0)
				end
				if(boss.timer >= 120) then
					boss.timer = 0
					boss.phase = 2
				end

			-- phase 2 (charge for a short while, then fire a huge laser)
			elseif(boss.phase == 2) then
				if(boss.timer == 60) then
					local s = {}
					s.x = boss.x - 12
					s.y = boss.y
					s.w = 16
					s.h = 16
					s.spd = 0
					s.spr = 78
					add(enemyshots,s)
					sfx(0)
					for i=0,6 do
						local s = {}
						s.x = boss.x - 20 - i*8
						s.y = boss.y
						s.w = 8
						s.h = 16
						s.spd = 0
						s.spr = 78
						add(enemyshots,s)
						sfx(0)
					end
				elseif(boss.timer == 76) then
					enemyshots = {}
					boss.timer = 0
					boss.phase = 3
				end

			-- phase 3 (shoot 3 seeker missiles)
			elseif(boss.phase == 3) then
				if(boss.timer % 12 == 0 and boss.timer <= 48) then
					local s = {}
					s.x = boss.x + 34
					s.y = boss.y + 8
					s.w = 8
					s.h = 8
					if(boss.health / boss.maxhealth > 0.5) then
						s.spd = -2
					else
						s.spd = -4
					end
					s.spr = 91
					s.seeking = true
					add(enemyshots,s)
					sfx(0)
				end
				if(boss.timer >= 75) then
					boss.timer = 0
					boss.phase = 1
				end
			end
		end


		-- update game.timer
		game.timer = (game.timer + 1)  % 32000
	end
end

